  "scripts": {
    "start": "gulp webpack-dev-server",
    "test": "react-snapshot-test-generator && jest --config jest.json",
    "build": "gulp build",
    "publish": "gulp publish",
    "clean": "gulp clean",
    "reinstall": "rm -rf node_modules && npm install",
    "lint": "eslint src --quiet --ext .js --ext .jsx",
    "lint-tests": "eslint test --quiet --ext .js --ext .jsx",
    "lint-all": "eslint src test --quiet --ext .js --ext .jsx --fix",
    "e2e": "./test.sh",
    "e2e-branch": "nightwatch -e chrome --suiteRetries 1 --skiptags socialLogins,demoApp,customSocket",
    "e2e-master-devel": "nightwatch -e chrome --suiteRetries 1 --skiptags demoApp,customSocket",
    "e2e-local": "nightwatch -e chrome --suiteRetries 1 --skiptags apns,administrators,customSocket --skipgroup billing,signup_logins,settings",
    "e2e-tag": "nightwatch -e chrome --suiteRetries 1 --tag",
    "iconfont": "gulp iconfont",
    "add-github-tag": "gulp add-github-tag",
    "check-github-tag": "gulp check-github-tag",
    "s3-cleanup": "gulp s3-cleanup",
    "deployment-master": "gulp deployment-master",
    "deployment-beta": "gulp deployment-beta",
    "deployment-devel": "gulp deployment-devel",
    "deployment-branch": "gulp deployment-branch",
    "git-config": "git config --global user.email \"ci@syncano.com\" && git config --global user.name \"CI\"",
    "predeployment-master": "npm version patch -m \"Release v%s [ci skip]\"",
    "postversion": "git push origin master && git push origin --tags"
  }
